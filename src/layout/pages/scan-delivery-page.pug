extends ../elements/layout-page
include ../templates/btn-panel
include ../templates/form/form-field-wrp-input

block data
  -
    title = "Scan: ready for delivery"

block content
  .scan-page__main
    .scan-page__content
      +btn-panel({activeSection: "Ready for Delivery"})

      .scan-page__form-wrp
        .scan-page__form-scan
          +form-field-wrp-input({
            placeholder: "Enter Barcode Numbers, Vendor ID, Equipments",
            color: "blue"
          })

    .scan-page__profile
